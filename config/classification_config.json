{
  "k-fold-splits": 200,
  "k-fold-train-size": 0.67,
  "input_data_folder": "FeatureExtraction/Output/csv/",
  "output_result_directory": "Classification/Output/raw/ba/",
  "output_ba_plot_directory": "Classification/Output/plots/ba/",
  "output_fi_plot_directory": "Classification/Output/plots/fi/",
  "ML-method-options": [
    "1vsA",
    "ML",
    "RR"
  ],
  "Classifier-options": [
    "KNN",
    "RF",
    "XGBoost"
  ],
  "XGBoost-options": {
    "ML": {
      "booster": "gblinear",
      "feature_selector": "shuffle",
      "updater": "shotgun",
      "lambda": 0.1
    },
    "1vsA": {
      "booster": "gblinear",
      "feature_selector": "shuffle",
      "updater": "coord_descent",
      "lambda": 0.1
    },
    "RR": {
      "booster": "gblinear",
      "feature_selector": "shuffle",
      "updater": "coord_descent",
      "lambda": 0.1
    }
  },
  "skip-features": [
    "virus",
    "protein",
    "protein_group",
    "sequence",
    "label"
  ]
}
