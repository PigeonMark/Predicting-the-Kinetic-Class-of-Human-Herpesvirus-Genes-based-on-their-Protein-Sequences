{
  "k-fold-splits": 200,
  "k-fold-train-size": 0.67,
  "input_data_folder": "FeatureExtraction/Output/csv/",
  "input_pca_folder": "FeatureExtraction/Output/pca/",
  "output_result_directory": "Classification/Output/raw/",
  "output_bar_plot_directory": "Classification/Output/plots/bar/",
  "output_roc_curves_plot_directory": "Classification/Output/plots/roc_curves/",
  "output_pi_plot_directory": "Classification/Output/plots/pi/",
  "output_pca_plot_directory": "Classification/Output/plots/pca/",
  "output_pca_variance_plot_directory": "Classification/Output/plots/pca_variance/",
  "filter_latent": true,
  "standardization": true,
  "pca_features": false,
  "n-pca": [
    null
  ],
  "ML-method-options": [
    "1vsA",
    "ML",
    "RR"
  ],
  "Classifier-options": [
    "KNN",
    "RF",
    "XGBoost"
  ],
  "XGBoost-options": {
    "ML": {
      "booster": "gblinear",
      "feature_selector": "shuffle",
      "updater": "coord_descent"
    },
    "1vsA": {
      "booster": "gblinear",
      "feature_selector": "shuffle",
      "updater": "coord_descent"
    },
    "RR": {
      "booster": "gblinear",
      "feature_selector": "shuffle",
      "updater": "coord_descent"
    }
  },
  "skip-features": [
    "virus",
    "protein",
    "protein_group",
    "sequence",
    "label"
  ]
}
